{
  "name": "my-workspace",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "watch-lib": "ng build custom-form-controls --watch --configuration development",
    "test:lib": "jest --detectOpenHandles --config ./projects/custom-form-controls/jest.config.js",
    "test:showcase": "jest --detectOpenHandles --config ./projects/form-playground/jest.config.js",
    "test": "npm run test:lib && npm run test:showcase",
    "test:lib:watch": "jest -o --watch --config ./projects/custom-form-controls/jest.config.js",
    "test:showcase:watch": "jest -o --watch --config ./projects/form-playground/jest.config.js",
    "test:lib:coverage": "jest --coverage --config ./projects/custom-form-controls/jest.config.js",
    "test:showcase:coverage": "jest --coverage --config ./projects/form-playground/jest.config.js"
  },
  "private": true,
  "dependencies": {
    "@angular-eslint/schematics": "^16.0.3",
    "@angular/animations": "^16.1.4",
    "@angular/common": "^16.1.4",
    "@angular/compiler": "^16.1.4",
    "@angular/core": "^16.1.4",
    "@angular/forms": "^16.1.4",
    "@angular/platform-browser": "^16.1.4",
    "@angular/platform-browser-dynamic": "^16.1.4",
    "@angular/router": "^16.1.4",
    "rxjs": "~7.8.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.4",
    "@angular/cli": "^16.1.4",
    "@angular/compiler-cli": "^16.1.4",
    "@types/jasmine": "~4.3.5",
    "@types/jest": "^29.5.2",
    "jasmine-core": "~5.0.1",
    "jest": "^29.6.1",
    "jest-preset-angular": "^13.1.1",
    "karma-coverage": "^2.2.1",
    "ng-packagr": "^16.1.0",
    "postcss": "^8.4.25",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.1",
    "typescript": "~5.1.6"
  }
}
